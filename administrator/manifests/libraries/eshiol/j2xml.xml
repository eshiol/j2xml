<?xml version="1.0" encoding="utf-8"?>
<extension type="library" method="upgrade">
	<name>J2XML</name>
	<libraryname>eshiol/J2xml</libraryname>
	<description>J2XML Library. Export/Import Joomla! content.</description>
	<author>Helios Ciancio</author>
	<authorEmail>info (at) eshiol (dot) it</authorEmail>
	<authorUrl>www.eshiol.it</authorUrl>
	<creationDate>__DEPLOY_DATE__</creationDate>
	<copyright><![CDATA[(C) 2010 - 2023 Helios Ciancio. All Rights Reserved.]]></copyright>
	<license>http://www.gnu.org/licenses/gpl-3.0.html GNU/GPL v3</license>
	<version>__DEPLOY_VERSION__</version>
	<files>
    	<filename>Exporter.php</filename>
		<filename>Importer.php</filename>
		<filename>Messages.php</filename>
		<filename>Sender.php</filename>
		<filename>Version.php</filename>
		<filename>classmap.php</filename>
		<folder>Table</folder>
	</files>
	<languages folder="language">
		<language tag="en-GB">en-GB/en-GB.lib_j2xml.ini</language>
		<language tag="en-GB">en-GB/en-GB.lib_j2xml.sys.ini</language>
	</languages>
	<media destination="lib_eshiol_j2xml" folder="media">
		<folder>js</folder>
	</media>
</extension>
